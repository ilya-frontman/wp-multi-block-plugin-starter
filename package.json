{
	"name": "wp-block-multi-start",
	"version": "0.1.0",
	"description": "Example block scaffolded with Create Block tool.",
	"author": "ilya frontman <ilya.frontman@yandex.ru>",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"scripts": {
		"build": "wp-scripts build",
		"format": "wp-scripts format --write \"**/*.(js|scss|*)\"",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start",
		"stylelint": "stylelint \"src/**/*.scss\" --cache",
		"stylelint:fix": "stylelint --fix \"src/**/*.scss\"",
		"eslint": "eslint \"src/**/*.js\" --cache --no-error-on-unmatched-pattern",
		"eslint:fix": "eslint --fix \"src/**/*.js\" --no-error-on-unmatched-pattern",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@babel/core": "^7.20.5",
		"@babel/eslint-parser": "^7.19.1",
		"@babel/preset-env": "^7.20.2",
		"@wordpress/babel-preset-default": "^7.7.0",
		"@wordpress/eslint-plugin": "^13.7.0",
		"@wordpress/prettier-config": "^2.6.0",
		"@wordpress/scripts": "^25.0.0",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.2",
		"lint-staged": "^13.1.0",
		"prettier": "^2.8.1",
		"sass": "^1.57.0",
		"stylelint": "^14.16.0",
		"stylelint-config-prettier": "^9.0.4",
		"stylelint-config-recess-order": "^3.0.0",
		"stylelint-config-standard-scss": "^6.1.0",
		"stylelint-order": "^5.0.0",
		"stylelint-prettier": "^2.0.0",
		"stylelint-scss": "^4.3.0"
	}
}
